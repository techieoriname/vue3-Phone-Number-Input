  "main": "dist/vue3-phone-input.umd.js",
  "module": "dist/vue3-phone-input.esm.js",
  "types": "dist/index.d.ts",
  "style": "dist/css/vue3-phone-input.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/techieoriname/vue3-Phone-Number-Input.git"
